    "preds = lm4.predict()\n",
    "errors = preds - np.log(df[\"predict\"])\n",
    "time_data = np.array([df.iloc[i].name.to_datetime() for i in range(len(df))]) #generate an array of dates \n",
    "plt.bar(time_data, errors)\n",
    "plt.ylabel(\"Errors\")\n",
    "plt.xlabel(\"Time\")\n",
    "plt.title(\"Errors vs Time plot\")"
